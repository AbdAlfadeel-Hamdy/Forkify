, this.toggleWindow.bind(this));\r\n    this._overlay.addEventListener('click', this.toggleWindow.bind(this));\r\n  }\r\n  addHandlerUpload(handler) {\r\n    this._parentElement.addEventListener('submit', function (e) {\r\n      e.preventDefault();\r\n      const dataArr = [...new FormData(this)];\r\n      const data = Object.fromEntries(dataArr);\r\n      handler(data);\r\n    });\r\n  }\r\n  generateMarkup() {\r\n    const html = `\r\n      <div class=\"upload__column\">\r\n        <h3 class=\"upload__heading\">Recipe data</h3>\r\n        <label>Title</label>\r\n        <input value=\"\" required name=\"title\" type=\"text\" />\r\n        <label>URL</label>\r\n        <input value=\"\" required name=\"sourceUrl\" type=\"text\" />\r\n        <label>Image URL</label>\r\n        <input value=\"\" required name=\"image\" type=\"text\" />\r\n        <label>Publisher</label>\r\n        <input value=\"\" required name=\"publisher\" type=\"text\" />\r\n        <label>Prep time</label>\r\n        <input value=\"\" required name=\"cookingTime\" type=\"number\" />\r\n        <label>Servings</label>\r\n        <input value=\"\" required name=\"servings\" type=\"number\" />\r\n      </div>\r\n      <div class=\"upload__column\">\r\n        <h3 class=\"upload__heading\">Ingredients</h3>\r\n        <label>Ingredient 1</label>\r\n        <input\r\n          value=\"\"\r\n          type=\"text\"\r\n          required\r\n          name=\"ingredient-1\"\r\n          placeholder=\"Format: 'Quantity,Unit,Description'\"\r\n        />\r\n        <label>Ingredient 2</label>\r\n        <input\r\n          value=\"\"\r\n          type=\"text\"\r\n          name=\"ingredient-2\"\r\n          placeholder=\"Format: 'Quantity,Unit,Description'\"\r\n        />\r\n        <label>Ingredient 3</label>\r\n        <input\r\n          value=\"\"\r\n          type=\"text\"\r\n          name=\"ingredient-3\"\r\n          placeholder=\"Format: 'Quantity,Unit,Description'\"\r\n        />\r\n        <label>Ingredient 4</label>\r\n        <input\r\n          type=\"text\"\r\n          name=\"ingredient-4\"\r\n          placeholder=\"Format: 'Quantity,Unit,Description'\"\r\n        />\r\n        <label>Ingredient 5</label>\r\n        <input\r\n          type=\"text\"\r\n          name=\"ingredient-5\"\r\n          placeholder=\"Format: 'Quantity,Unit,Description'\"\r\n        />\r\n        <label>Ingredient 6</label>\r\n        <input\r\n          type=\"text\"\r\n          name=\"ingredient-6\"\r\n          placeholder=\"Format: 'Quantity,Unit,Description'\"\r\n        />\r\n      </div>\r\n\r\n      <button class=\"btn upload__btn\">\r\n        <svg>\r\n          <use href=\"src/img/icons.svg#icon-upload-cloud\"></use>\r\n        </svg>\r\n        <span>Upload</span>\r\n      </button>\r\n    `;\r\n    this._parentElement.innerHTML = '';\r\n    this._parentElement.insertAdjacentHTML('beforeend', html);\r\n  }\r\n}\r\n\r\nexport default new AddRecipeView();\r\n"},"lineCount":null}},"error":null,"hash":"fa720ebbd7211eeb789d0e1f3ad847c8","cacheData":{"env":{}}}